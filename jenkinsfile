pipeline{
    agent any
    stages{
        stage('Checkout'){
            steps{
                echo 'Checking out the code...'
            }
        }
        stage('Pruebas de SAST'){
            steps{
                echo 'Testing the application...'
            }
        }
        stage('Build'){
            steps{
                echo 'Building the application...'
            }
        }
    }
}