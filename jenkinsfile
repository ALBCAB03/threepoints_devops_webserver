pipeline{
    agent any
  
    stages{
        stage('Checkout'){
            steps{
                git branch: 'master', url: 'https://github.com/ALBCAB03/threepoints_devops_webserver.git'
            }
        }
        stage('Pruebas de SAST'){
            parallel{
                stage('Ejecutar SAST'){
                    steps{
                        echo 'Iniciando pruebas de SAST'
                    }
                }
                stage('Imprimir Env'){
                    steps{
                        echo 'WORKSPACE: ${env.WORKSPACE}'
                    }
                }
            }
        }
        stage('Build'){
            steps{
                sh 'docker build -t devops_ws .'
            }
        }
    }
}